package com.test;

public class Test 
{
	public static void main(String[] args) {
		
		int [] arr=new int[] {13,46,24,52,20,9};
		selectionSort(arr);
		
	}
	
	public static void selectionSort(int[] arr)
	{
		for(int i=0;i<arr.length-1;i++)
		{
			int min =arr[i];
			for(int j=i;j<arr.length;j++)
			{
				if(arr[j]<min)
				{
					
				}
			}
		}
		
	}

}
