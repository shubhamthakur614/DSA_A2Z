package com.basic.sorting;

public class SelectionSort {
	public static void main(String[] args) {

		int[] arr = new int[] { 13, 46, 24, 52, 20, 9 };

		for (int i = 0; i < arr.length; i++) {
			int min=arr[i];
			for (int j = i + 1; j < arr.length; j++) {

				if(min>arr[j])
				{
					min=j;
				}	
			}
			
			System.out.println(min);
		}

	}

}
